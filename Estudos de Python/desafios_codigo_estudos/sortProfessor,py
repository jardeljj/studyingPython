lista = []
i = 1

while (i<=5):
  atual = int(input('Digite um numero: '))  
  lista.append(atual)  
  i = i + 1  

i = 0
cont = 1

while(cont <= len(lista)):  
  i = 0

  while(i < len(lista)):  
       
    atual = lista[i]
   
    if i < (len(lista) - 1):
        proximo = lista[i+1]
   
        if proximo < atual:

          lista[i] = proximo
          lista[i+1] = atual
           
    i += 1
 
  cont = cont + 1
 
print(lista)
